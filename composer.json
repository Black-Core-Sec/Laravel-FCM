{
    "name": "peacerevolution/laravel-fcm",
    "description": "Laravel / Lumen package for Firebase Cloud Messaging ",
    "keywords": [
        "laravel",
        "lumen",
        "firebase",
        "notification",
        "push",
        "fcm",
        "firebase cloud messaging"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Brosy",
            "email": "nicolas.brosy@gmail.com"
        },
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "monolog/monolog": "^1.0|^2.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^9.4.3",
        "php-coveralls/php-coveralls": "^2.5.2",
        "laravel/laravel": "9.0.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelFCM\\": "src/",
            "LaravelFCM\\Mocks\\": "tests/mocks"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "replace": {
        "brozot/laravel-fcm": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFCM\\FCMServiceProvider"
            ],
            "aliases": {
                "FCM": "LaravelFCM\\Facades\\FCM",
                "FCMGroup": "LaravelFCM\\Facades\\FCMGroup"
            }
        }
    }
}